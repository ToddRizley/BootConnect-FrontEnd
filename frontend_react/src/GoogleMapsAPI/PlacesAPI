

import axios from 'axios'

function fetchLatLong(location){
  const API_KEY = "AIzaSyByc29-KtwjrhnW6hwmyXeeWKyi2Asumyw"
  let city = location["city"]
  let state = location["state"]
  const url = 'https://maps.googleapis.com/maps/api/geocode/json?address=' + city + "+" + state + "&key=" URL
  const request = axios.get(url)
  console.log('request', request)

  return {
    type: 'FETCH_LAT_LONG',
    payload: request
  }
}
export default fetchLatLong
